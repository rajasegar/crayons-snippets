{
  "name": "crayons-snippets",
  "displayName":"Freshworks Crayons Snippets",
  "version": "1.0.0",
  "publisher": "Rajasegar",
  "description": "VSCode and Neovim snippets for Freshworks Crayons",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "node generate.js"
  },
  "keywords": ["snippets","snippet","vscode","neovim","crayons","freshworks"],
  "author": "Rajasegar Chandran",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajasegar/crayons-snippets"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/webcomponents.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.code-snippets"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ]
}
